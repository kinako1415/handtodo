{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "gesture-todo-app",
  "version": "0.1.0",
  "identifier": "echo "com.gesturetodo.app" | npm create tauri-app@latest gesture-todo-app -- --template react-ts",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "gesture-todo-app",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
